<h1>New user</h1>
<%= form_for @user do |f| %>
<%= f.label "Username" %>
<%= f.text_field :user_name %><br>
<%= f.label "Full Name" %>
<%= f.text_field :name %><br>
<%= f.label "Age" %>
<%= f.number_field :age %><br>
<%= f.label "Birthday" %>
<%= f.date_field :birthday %><br>

<%= f.submit "Create Account" %>
<% end %>